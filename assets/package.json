{
	"name": "ismailivolunteer",
	"version": "0.1.0",
	"description": "IsmailiVolunteer",
	"main": "none",
	"private": true,
	"scripts": {
		"modernizr": "./node_modules/.bin/modernizr -c modernizr-config.json -d ../priv/static/js",
		"rollup": "./node_modules/.bin/rollup -c ./rollup.config.js",
		"scripts": "npm run modernizr && npm run rollup",
		"scss": "./node_modules/.bin/node-sass-chokidar ./css --output ../priv/static/css --include-path ./node_modules --output-style compressed",
		"styles": "npm run scss && ./node_modules/.bin/postcss ../priv/static/css/*.css --replace --use autoprefixer",
		"fontawesome": "cp -r node_modules/@fortawesome/fontawesome-pro/webfonts/. ./static/fonts",
		"fonts": "npm run fontawesome",
		"static": "cp -r ./static/. ../priv/static",
		"minify_scripts": "find . -path '*priv/static/js*' -name '*.js' -exec ./node_modules/.bin/uglifyjs '{}' -o '{}' ';'",
		"minify": "npm run minify_scripts",
		"clean": "rm -rf ../priv/static",
		"compile": "npm run clean && npm run scripts && npm run styles && npm run fonts && npm run static",
		"production": "npm run compile && npm run minify"
	},
	"author": "Alizain Feerasta",
	"license": "UNLICENSED",
	"devDependencies": {
		"@babel/cli": "^7.1.5",
		"@babel/core": "^7.1.6",
		"@babel/plugin-external-helpers": "^7.0.0",
		"@babel/preset-env": "^7.3.1",
		"autoprefixer": "^9.4.6",
		"del": "^3.0.0",
		"eslint": "^5.12.1",
		"eslint-config-airbnb-base": "^13.1.0",
		"eslint-plugin-compat": "^2.6.3",
		"eslint-plugin-import": "^2.15.0",
		"eslint-plugin-promise": "^4.0.1",
		"html-minifier": "^3.4.2",
		"minimist": "^1.2.0",
		"mkdirp": "^0.5.1",
		"moment": "^2.24.0",
		"node-sass-chokidar": "^1.3.3",
		"postcss": "^7.0.14",
		"postcss-cli": "^6.0.1",
		"rollup": "^1.1.2",
		"rollup-plugin-babel": "^4.3.2",
		"rollup-plugin-commonjs": "^9.2.0",
		"rollup-plugin-node-resolve": "^4.0.0",
		"rollup-watch": "^4.3.1",
		"uglify-js": "^3.4.9"
	},
	"dependencies": {
		"@fortawesome/fontawesome-pro": "^5.7.1",
		"bootstrap": "4.2.1",
		"iframe-resizer": "^3.6.4",
		"jquery": "^3.1.1",
		"jump.js": "^1.0.2",
		"lodash.throttle": "^4.1.1",
		"modernizr": "^3.4.0",
		"parsleyjs": "^2.6.2",
		"phoenix": "file:../deps/phoenix",
		"phoenix_html": "file:../deps/phoenix_html",
		"popper.js": "^1.14.4",
		"url-search-params-polyfill": "^5.0.0"
	}
}
