{
	"name": "ismailivolunteer",
	"version": "0.1.0",
	"description": "IsmailiVolunteer",
	"main": "none",
	"private": true,
	"scripts": {
		"modernizr": "./node_modules/.bin/modernizr -c modernizr-config.json -d ../priv/static/js",
		"rollup": "./node_modules/.bin/rollup -c ./rollup.config.js",
		"scripts": "npm run modernizr && npm run rollup",
		"scss": "./node_modules/.bin/sass --load-path=./node_modules ./css:../priv/static/css",
		"styles": "npm run scss && ./node_modules/.bin/postcss ../priv/static/css/**/*.css --replace --map --use cssnano --use autoprefixer",
		"fontawesome": "cp -r node_modules/@fortawesome/fontawesome-pro/webfonts/. ./static/fonts",
		"fonts": "npm run fontawesome",
		"static": "cp -r ./static/. ../priv/static",
		"minify_scripts": "find . -path '*priv/static/js*' -name '*.js' -exec ./node_modules/.bin/uglifyjs '{}' -o '{}' ';'",
		"minify": "npm run minify_scripts",
		"clean": "rm -rf ../priv/static",
		"compile": "npm run clean && npm run scripts && npm run styles && npm run fonts && npm run static",
		"production": "npm run compile && npm run minify"
	},
	"author": "Alizain Feerasta",
	"license": "UNLICENSED",
	"devDependencies": {
		"@babel/cli": "^7.8.4",
		"@babel/core": "^7.8.4",
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/preset-env": "^7.8.4",
		"autoprefixer": "^9.7.4",
		"cssnano": "^4.1.10",
		"cssnano-preset-default": "^4.0.7",
		"eslint": "^6.8.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-compat": "^3.5.1",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-promise": "^4.0.1",
		"postcss-cli": "^7.1.0",
		"rollup": "^1.31.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-watch": "^4.3.1",
		"sass": "^1.25.0",
		"uglify-js": "^3.7.7"
	},
	"dependencies": {
		"@fortawesome/fontawesome-pro": "^5.12.0",
		"@stimulus/polyfills": "^1.1.1",
		"bootstrap": "4.4.1",
		"dayjs": "^1.8.20",
		"jquery": "^3.1.1",
		"lodash.throttle": "^4.1.1",
		"modernizr": "^3.9.0",
		"phoenix": "file:../deps/phoenix",
		"phoenix_html": "file:../deps/phoenix_html",
		"popper.js": "^1.16.1",
		"stimulus": "^1.1.1",
		"url-search-params-polyfill": "^7.0.1"
	}
}
