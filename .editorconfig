root = true

[*]
end_of_line = lf
insert_final_newline = true
charset = utf-8
indent_style = space
indent_size = 2

[*.py]
indent_style = tab
indent_size = 4

[*.{html,scss,json}]
indent_style = tab
indent_size = 4

[*.{md,yaml,yml}]
indent_style = space
indent_size = 2

[*.{js,jsx}]
indent_style = tab
indent_size = 2

[Vagrantfile]
indent_style = space
indent_size = 2
