<%= render "bare.html" do %>
	<h4 class="mb-1q"><%= assigns[:subtitle] %></h4>
	<ul class="item-meta list-inline font-family-sans-serif mb-0">
		<%= assigns[:meta] %>
	</ul>
	<ul class="nav nav-tabs justify-content-center font-family-sans-serif mt-5q">
		<%= for {title, path, active_class} <- assigns[:nav_items] do %>
			<li class="nav-item">
				<%= link title, to: path, class: "#{active_class} px-2 nav-link" %>
			</li>
		<% end %>
	</ul>
<% end %>
