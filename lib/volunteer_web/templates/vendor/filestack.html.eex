<script type="text/javascript" src="https://static.filestackapi.com/filestack-js/3.x.x/filestack.min.js"></script>
<script type="text/javascript">
	(function(window) {
		"use strict";

		window.FilestackClient = filestack.init("<%= @filestack_api_key %>", {
			security: {
				policy: "<%= @filestack_security.policy %>",
				signature: "<%= @filestack_security.signature %>",
			}
		});
	})(window);
</script>
